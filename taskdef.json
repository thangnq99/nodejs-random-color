{
    "executionRoleArn": "arn:aws:iam::438465164455:role/udemy-devops-code-deploy-role",
    "containerDefinitions": [
        {
            "name": "nodejs-container",
            "image": "<IMAGE_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "containerPort": 3000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "nodejs-task-definition"
}